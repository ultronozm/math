}

@article{2023arXiv2312.10026,
  title                    = {A new lower bound for sphere packing},
  author                   = {Marcelo Campos and Matthew Jenssen and Marcus Michelen and Julian Sahasrabudhe},
  year                     = {2023},
  month                    = {12},
  url                      = {http://arxiv.org/abs/2312.10026v1},
  note                     = {arXiv:2312.10026},
  eprinttype               = {arXiv},
  abstract                 = { We show there exists a packing of identical spheres in $\mathbb{R}^d$ with
density at least
 \[
