@article{2023arXiv2307.10109,
  title                    = {Bad is null},
  author                   = {Victor Beresnevich and Shreyasi Datta and Anish Ghosh and Benjamin Ward},
  year                     = {2023},
  month                    = {07},
  url                      = {http://arxiv.org/abs/2307.10109v1},
  note                     = {arXiv:2307.10109},
  eprinttype               = {arXiv},
  abstract                 = { In this paper we develop a general framework of badly approximable points in
a metric space $X$ equipped with a $\sigma$-finite doubling Borel regular
measure $\mu$. We establish that under mild assumptions the $\mu$-measure of
the set of badly approximable points is always zero. The framework can be
applied to a variety of settings in Diophantine approximation and dynamical
systems, which we also consider, including weighted and $S$-arithmetic
Diophantine approximations, Diophantine approximation on manifolds and
intrinsic approximations on fractals.},
